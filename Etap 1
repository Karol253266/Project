#include <iostream>
using namespace std;
int main()
{

	int liczba_probowek;
	cout << "Podaj wartosc n" << endl;
	cin >> liczba_probowek;
	switch (liczba_probowek)
	{
	case 6:
	{
		cout << "robie instrukcje dla ilosci probowek rowna 6" << endl;
		for (int i = 0; i < 6; i++)
		{
			cout << "Probowka nr " << i + 1 << " zostala przebadana" << endl;
			
		}
		break;
		
	default: {
		cout << "wybrano zla ilosc probowek do przebadania. System nie posiada instrukcji do innej ilosci probowek." << endl;
		break;
	}
	}
	}
	system("pause");
}
